{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://gitlab.com/*merge_requests/*/diffs*"],
      "css": ["tree.css"],
      "js": ["diffsPageDom.js", "observeChangedFiles.js", "main.js"]
    }
  ]
}